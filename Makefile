DOTO	=		$(SRC:.c=.o)

DOTO_PF	=		$(SRC_PF:.c=.o)

#NAME
NAME=		libft.a

#RULE
all: $(NAME)

$(NAME): $(DOTO) $(DOTO_PF)
	ar rc $(NAME) $(DOTO) $(DOTO_PF)
	ranlib $(NAME)

%.o: %.c
	gcc -c $(FLAGS) -I includes $< -o $@

clean:
	rm -rf $(DOTO) $(DOTO_PF)

fclean:	clean
	rm -rf $(NAME)

re: fclean $(NAME)
